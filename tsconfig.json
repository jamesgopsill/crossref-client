{
	"$schema": "https://json.schemastore.org/tsconfig",
	"extends": "@tsconfig/node18-strictest-esm/tsconfig.json",
	"include": ["src"],
	"compilerOptions": {
		"lib": ["DOM", "ES2022"],
		"outDir": "dist/esm",
		"rootDir": "src",
		"declaration": true,
		"newLine": "LF",
		"moduleResolution": "node16"
	}
}
